--5-1. 查询 -> 比较谓词: between
select * from t1 where int64 between 300 and 600 order by time;
# 补充更多的数据类型
# 以及不支持的数据类型的报错
# limit + offset 结合的语句，确定是否支持

--5-2. 查询 -> 比较谓词: not between
select * from t1 where int64 not between 300 and 600 order by time;
# 补充更多的数据类型
# 以及不支持的数据类型的报错
# limit + offset 结合的语句，确定是否支持


--5-3. 查询 -> 比较谓词: is null
select * from t1 where text is null order by time;
# 补充limit
# 补充where 时间过滤

--5-4. 查询 -> 比较谓词: is not null
select * from t1 where text is not null order by time;
# 补充limit
# 补充where 时间过滤、值过滤(不同列的值过滤)，例如select * from t1 where text is not null and int32=1;


--5-5. 查询 -> 比较谓词: in
select * from t1 where text in ('aaa','bbb','ccc') order by time;
# 补充limit

--5-6. 查询 -> 比较谓词: not in
select * from t1 where text not in ('aaa','bbb','ccc') order by time;
# 补充limit


---10. 混合 null 和 like
select * from t1 where text is null or text like 'aaa' order by time;


--11. 清理数据: 删除数据库 
