// 比较谓词

connect root/root;
--0.清理数据: 删除数据库 
drop database test_db;
<<NULL;

--1. 创建数据库
create database test_db;

--2. 指定数据库
use test_db;

--3. 创建表 t1
create table t1
(
    "region" STRING ID,
    "factoryid" STRING ID,
    "deviceid" STRING ID,
    "type" STRING ATTRIBUTE,
    "int64" INT64,
    "text" TEXT,
    "boolean" BOOLEAN,
    "int32" INT32,
    "int64_2" INT64,
    "double" DOUBLE
);

